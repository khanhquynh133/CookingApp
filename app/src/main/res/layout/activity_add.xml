<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageButton
        android:id="@+id/btn_drink2"
        android:layout_width="90dp"
        android:layout_height="75dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="39dp"
        android:background="@drawable/btn_cakedrinkfood"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_food7"
        app:srcCompat="@drawable/drink"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_categories2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="261dp"
        android:fontFamily="@font/poppinssemibold"
        android:text="Categories"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_categories3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_categories4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="124dp"
        android:fontFamily="@font/poppinssemibold"
        android:text="Cake"
        android:textSize="17sp"
        app:layout_constraintStart_toEndOf="@+id/btn_cake7"
        app:layout_constraintTop_toBottomOf="@+id/tv_categories3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_categories5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="87dp"
        android:fontFamily="@font/poppinssemibold"
        android:text="Food"
        android:textSize="17sp"
        app:layout_constraintStart_toEndOf="@+id/btn_food7"
        app:layout_constraintTop_toBottomOf="@+id/tv_categories4"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_categories6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="85dp"
        android:fontFamily="@font/poppinssemibold"
        android:text="Drink"
        android:textSize="17sp"
        app:layout_constraintStart_toEndOf="@+id/btn_drink2"
        app:layout_constraintTop_toBottomOf="@+id/tv_categories5"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_categories3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="168dp"
        android:fontFamily="@font/poppinssemibold"
        android:text="ADD RECEIPE"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_hello2"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/btn_cake7"
        android:layout_width="90dp"
        android:layout_height="75dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/btn_cakedrinkfood"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_categories2"
        app:srcCompat="@drawable/cake"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/btn_food7"
        android:layout_width="90dp"
        android:layout_height="75dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/btn_cakedrinkfood"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_cake7"
        app:srcCompat="@drawable/food"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_hello2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="64dp"
        android:fontFamily="@font/poppinssemibold"
        android:text="Hello hoangquynh2000 "
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/ava2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/ava2"
        android:layout_width="57dp"
        android:layout_height="60dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/btn_circle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ava"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/btn_addfood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="92dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/tv_categories6"
        app:layout_constraintTop_toBottomOf="@+id/btn_adddrink"
        app:srcCompat="@drawable/addicon"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/btn_adddrink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="92dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/tv_categories5"
        app:layout_constraintTop_toBottomOf="@+id/btn_addcake"
        app:srcCompat="@drawable/addicon"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/btn_addcake"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="41dp"
        android:layout_marginTop="296dp"
        android:layout_marginEnd="104dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_categories4"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/addicon"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomnav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:itemIconTint="@drawable/selector"
        app:itemTextColor="@drawable/selector"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/menu_nav"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>